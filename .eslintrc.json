{
  "env": {
    "browser": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["react", "react-hooks", "const-case", "@typescript-eslint"],
  "rules": {
    "no-console": "warn",
    "no-debugger": "warn",
    "quotes": ["error", "single"],
    "jsx-quotes": ["error", "prefer-single"],
    "no-var": "error",
    "const-case/uppercase": "error",
    "eqeqeq": "error",
    "no-unused-vars": "warn"
  }
}
